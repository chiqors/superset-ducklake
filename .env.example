# Superset Secrets
SUPERSET_SECRET_KEY=replace_with_secure_key_generated_by_openssl

# Admin Credentials
ADMIN_USERNAME=admin
ADMIN_PASSWORD=admin
ADMIN_EMAIL=admin@superset.com

# Database Credentials
POSTGRES_USER=superset
POSTGRES_PASSWORD=superset
POSTGRES_DB=ducklake_analytics

# Valkey (Redis) Configuration (Production Mode Only)
REDIS_HOST=valkey
REDIS_PORT=6379
REDIS_CELERY_DB=0
REDIS_RESULTS_DB=1
REDIS_CACHE_DB=2

# GCS DuckLake Configuration
GCS_KEY_ID=your_gcs_access_key
GCS_SECRET=your_gcs_secret
GCS_DATA_PATH=gs://your-bucket/path/

