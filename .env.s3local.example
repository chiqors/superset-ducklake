# Superset Secrets
SUPERSET_SECRET_KEY=replace_with_secure_key_generated_by_openssl

# Admin Credentials
ADMIN_USERNAME=admin
ADMIN_PASSWORD=admin
ADMIN_EMAIL=admin@superset.com

# Superset Metadata Database (Postgres)
POSTGRES_USER=analyst
POSTGRES_PASSWORD=analyst
POSTGRES_DB=superset

# DuckLake Metadata Database (Postgres)
# Can be the same instance/user as Superset, but usually a different DB name
POSTGRES_DUCKLAKE_USER=analyst
POSTGRES_DUCKLAKE_PASSWORD=analyst
POSTGRES_DUCKLAKE_HOST=postgres
POSTGRES_DUCKLAKE_PORT=5432
POSTGRES_DUCKLAKE_DB=ducklake_analytics

# Valkey (Redis) Configuration (Production Mode Only)
REDIS_HOST=valkey
REDIS_PORT=6379
REDIS_CELERY_DB=0
REDIS_RESULTS_DB=1
REDIS_CACHE_DB=2

# DuckLake Storage Configuration
# Choose storage driver: 'gcs' or 's3'
DUCKLAKE_STORAGE_DRIVER=s3

# 1. GCS Configuration (Required if DUCKLAKE_STORAGE_DRIVER=gcs)
# GCS_KEY_ID=your_gcs_access_key
# GCS_SECRET=your_gcs_secret
# GCS_BUCKET_PATH=gs://your-bucket/path/

# 2. S3 / MinIO Configuration (Required if DUCKLAKE_STORAGE_DRIVER=s3)
# If using AWS S3, leave ENDPOINT commented or empty
# If using MinIO, set ENDPOINT to your MinIO URL (e.g. s3.amazonaws.com or minio:9000)
S3_ACCESS_KEY_ID=rustfsadmin
S3_SECRET_ACCESS_KEY=rustfsadmin
S3_BUCKET_PATH=s3://analitycs/
S3_ENDPOINT=rustfs:9000
S3_REGION=us-east-1
S3_URL_STYLE=path  # Use 'path' for MinIO/GCS, 'vhost' for AWS S3
S3_USE_SSL=false

# MotherDuck Configuration (Optional)
# MOTHERDUCK_TOKEN=

# BigQuery Configuration (optional)
# Set to true to enable BigQuery support via DuckDB BigQuery plugin
BIGQUERY_ENABLED=true
# Your GCP project ID - all datasets in this project will be accessible
BIGQUERY_PROJECT_ID=oucru-id-cloud
# Path to service account JSON file (will be mounted from ./secrets/)
GOOGLE_APPLICATION_CREDENTIALS=/app/secrets/bigquery-sa.json